    <command:details>
      <command:name>Invoke-HKAVRCommand</command:name>
      <command:verb>Invoke</command:verb>
      <command:noun>HKAVRCommand</command:noun>
      <maml:description>
        <maml:para>Sends a command to the AVR.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>This method sends a specific SOAP request to the specified Harman Kardon AVR controller.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Invoke-HKAVRCommand</maml:name>
