          <maml:name>Parameter</maml:name>
          <maml:description>
            <maml:para>Verwenden Sie diesen Parameter, um zusätzliche Parameter für den Befehl mitzugeben.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
      </command:syntaxItem>
