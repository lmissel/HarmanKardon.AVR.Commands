    <command:examples>
      <command:example>
        <maml:title>-------------------------- EXAMPLE 1 --------------------------</maml:title>
        <dev:code>Invoke-HKAVRCommand -HostName 172.16.15.123 -Port 10025 -Command "power-on" -Zone "Main Zone"</dev:code>
        <dev:remarks>
          <maml:para>Dieser Befehl schaltet einen AVR-Controller vom Zustand "Standby" in den Zustand "Power-On".</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- EXAMPLE 2 --------------------------</maml:title>
        <dev:code>Invoke-HKAVRCommand -HostName 172.16.15.123 -Port 10025 -Command "power-off" -Zone "Main Zone"</dev:code>
        <dev:remarks>
          <maml:para>Dieser Befehl schaltet einen AVR-Controller vom Zustand "Power-On" in den Zustand "Power-Off".</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- EXAMPLE 3 --------------------------</maml:title>
        <dev:code>Invoke-HKAVRCommand -HostName 172.16.15.123 -Port 10025 -Command "source-selection" -Zone "Main Zone" -Parameter "AUX"</dev:code>
        <dev:remarks>
          <maml:para>Dieser Befehl schaltet die Quelle des AVR-Controllers auf AUX um.</maml:para>
        </dev:remarks>
      </command:example>
