<?xml version="1.0" encoding="utf-8"?>
<helpItems schema="maml" xmlns="http://msh">
  <command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10" xmlns:MSHelp="http://msdn.microsoft.com/mshelp">
    <command:details>
      <command:name>Invoke-HKAVRCommand</command:name>
      <command:verb>Invoke</command:verb>
      <command:noun>HKAVRCommand</command:noun>
      <maml:description>
        <maml:para>Sendet einen Befehl an einem AVR Controller.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>Diese Methode sendet einen SOAP-Request an den angegebenen Harman Kardon AVR Controller.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Invoke-HKAVRCommand</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="True (ByPropertyName)" position="1" aliases="none">
          <maml:name>HostName</maml:name>
          <maml:description>
            <maml:para>Verwenden Sie diesen Parameter, um den Hostnamen oder die IP-Adresse des AVR-Controllers einzustellen.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="True (ByPropertyName)" position="2" aliases="none">
          <maml:name>Port</maml:name>
          <maml:description>
            <maml:para>Geben Sie den Port an, auf dem der SOAP-Webdienst erreicht werden kann. Standard ist: "10025".</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">Int32</command:parameterValue>
          <dev:type>
            <maml:name>Int32</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>10025</dev:defaultValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="True (ByPropertyName)" position="3" aliases="none">
          <maml:name>Command</maml:name>
          <maml:description>
            <maml:para>Gibt den ausführenden Befehl an.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="True (ByPropertyName)" position="4" aliases="none">
          <maml:name>Zone</maml:name>
          <maml:description>
            <maml:para>Verwenden Sie diesen Parameter, um die Zone anzugeben, in der der Befehl ausgeführt werden soll. Voreinstellung ist: "Main-Zone".</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
